{
  "manifest_version": 2,
  "name": "Automa",
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-128.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ]
}
